<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pokidin.a.kava">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/kava_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/kava_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".KavaListActivity" />
        <activity
            android:name=".KavaActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".TopLevelActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>